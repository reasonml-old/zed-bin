{
  "name": "zed-bin",
  "version": "1.4.0",
  "description": "bin wrapper for zed",
  "license": "MIT",
  "scripts": {
    "postinstall": "eval $(dependencyEnv) && nopam && make install"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "exportedEnvVars": {
      "PATH": {
          "global": true,
          "resolveAsRelativePath": true,
          "globalCollisionBehavior": "joinPath",
          "val": "./_build/ocamlfind/bin"
      },
      "version": {
          "global": true,
          "globalCollisionBehavior": "clobber",
          "val": "1.4.0"
      },
      "zed_version": {
          "global": true,
          "globalCollisionBehavior": "clobber",
          "val": "1.4.0"
      },
      "FINDLIB": {
          "global": true,
          "resolveAsRelativePath": true,
          "globalCollisionBehavior": "joinPath",
          "val": "./_build/ocamlfind/lib"
      }
  },
  "dependencies": {
      "react-bin": "https://github.com/reasonml/react-bin",
      "@opam-alpha/ocamlfind": "*",
      "camomile-bin": "https://github.com/reasonml/camomile-bin",
      "dependency-env": "https://github.com/reasonml/dependency-env.git",
      "nopam": "https://github.com/reasonml/nopam.git"
  }
}
